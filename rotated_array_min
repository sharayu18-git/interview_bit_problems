public class Solution {
	// DO NOT MODIFY THE LIST
	public int findMin(final List<Integer> a) {

		int n = a.size();
		int low =0, high = n-1;
		while(low<=high){
			int mid = low + (high - low) /2;
			 if((mid>0  && a.get(mid-1)> a.get(mid)))
         		return a.get(mid);
        	 if(mid<n-1 && a.get(mid) > a.get(mid+1))
         		return a.get(mid+1);
        	 if(a.get(mid)<a.get(n-1)) high=mid-1;
        	else low=mid+1;
		}
		return a.get(0);
	}
}
